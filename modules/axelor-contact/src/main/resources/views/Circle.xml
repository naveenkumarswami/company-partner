<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="circle-grid" title="Circles" model="com.axelor.partner.db.Circle">
    <field name="code" title="Code"/>
    <field name="name" title="Name"/>
  </grid>

  <form name="circle-form" title="Circle" model="com.axelor.partner.db.Circle" width="mini">
    <panel title="Overview" itemSpan="12">
      <field name="code" title="Code"/>
      <field name="name" title="Circle Name"/>
    </panel>
  </form>

</object-views>
