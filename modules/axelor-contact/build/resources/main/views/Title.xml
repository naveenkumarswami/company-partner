<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="title-grid" title="Title" model="com.axelor.partner.db.Title">
    <field name="code" title="Code"/>
    <field name="name" title="Name"/>
  </grid>

  <form name="title-form" title="title" model="com.axelor.partner.db.Title" width="mini">
    <panel title="Title" itemSpan="12">
      <field name="code" title="Code"/>
      <field name="name" title="title Name"/>
    </panel>
  </form>

</object-views>
