<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="circle-grid" title="Circles" model="com.axelor.contact.db.Circle1">
    <field name="name" />
    <field name="code" />
  </grid>

  <form name="circle-form" title="Circle" model="com.axelor.contact.db.Circle1" width="mini">
    <panel title="Circles" itemSpan="12">
      <field name="code" canSuggest="true" help="this is code"/>
      <field name="name"/>
    </panel>
  </form>

</object-views>
